#include<iostream>
using namespace std;

class array
{
	int num[20];
	int n;

	void insert()
	{
		cout<<"\n\tEnter the number of elements";
		cin>>n;
		num[n];
		for(int i=0;i<n;i++)
		{
			cin>>num[i];
		}
		cout<<"\n\tEntered array is :";
		for(int i=0;i<n;i++)
		{
		    cout<<num[i];
		}

	}

	void bubblesort()
	{
		int temp;
		for(int i=n-1;i>=1;i--)
		{
			for(int j=0;j<i;j++)
			{
				if(num[j]>num[j+1])
				{
					temp=num[j];
					num[j]=num[j+1];
					num[j+1]=temp;

				}
			}
		}

	}


	void

};

int main()
{





	return 0;
}
