#include <iostream>
#include<string.h>
using namespace std;

class node
{
	int PRN;
	char name[20];
	node *next;
public:
	node(char *nm,int x)
	{
		PRN=x;
		strcpy(name,nm);
		next=NULL;

	}
};


int main()
{

	return 0;
}
